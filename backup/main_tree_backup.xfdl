    <Script type="xscript5.1"><![CDATA[/*
 본 프로젝트는 넥사크로플랫폼 17 개발가이드의 예제로 사용된 샘플 모음입니다.
 모든 샘플은 독립적인 Form 단위로의 동작에 대해서만 고려되었기 때문에
 통합된 하나의 애플리케이션으로 동작하지 않습니다.
 따라서 예제를 실행하려면 각 Form(xfdl) 별로 QuickView를 사용하시기 바라며
 Launch 실행은 권장하지 않습니다.
  */

var g_strAppInfo = nexacro.getApplication();
var strCategory = "";
var strSubject = "";
var strSampleFile = "";
var strDocID = "";

this.fn_displayInfo = function()
{
	/*
	if(!nexacro._manual && !nexacro._sample)
	{		
		alert(" 본 프로젝트는 넥사크로플랫폼 17 개발가이드의 예제로 사용된 \n\
 샘플 모음입니다. 모든 샘플은 독립적인 Form 단위로의 동작에 대해서만 \n\
 고려되었기 때문에 통합된 하나의 애플리케이션으로 동작하지 않습니다.\n\
 따라서 예제를 실행하려면 각 Form(xfdl) 별로 QuickView를 사용하시기 \n\
 바라며 Launch 실행은 권장하지 않습니다.");
		//g_strAppInfo.exit();
	}	
	*/	

	trace("OS: " + system.osversion);
	trace("Browser: " + system.navigatorname + " " + system.navigatorversion);
	trace("Engine version: " + g_strAppInfo.engineversion);
	trace("Applilcation version: " + g_strAppInfo.version);

	var titleComment = "본 예제는 " + g_strAppInfo.appVersion + " 버전에서 테스트되었습니다." 
		+ "\n" 
		+ "사용하는 제품의 테마에 따라 예제에 보이는 이미지와 실제 동작하는 애플리케이션이 다를 수 있습니다.";
	this.Div00.form.Static00.set_text(titleComment);

}

this.fn_LoadContents = function(strDocID, strSampleFile, strCategory, strSubject)
{
	var strServiceURL = g_strAppInfo.serverURL + "/17/Service/viewsource.jsp?fileName=";

	this.Div04.form.Static01.set_text(strCategory + " > " + strSubject);	

	this.Div01.form.WebBrowser00.set_url(g_strAppInfo.docsURL + strDocID);
	this.Div03.form.Tab00.tp_description.set_url("Sample::" + strSampleFile + ".xfdl");

	this.ds_source.clear();
	this.transaction( "tr"
					, strServiceURL + strSampleFile
					, ""
					, "ds_source=output"
					, ""
					, "fn_callback"
					, true); 
}

this.fn_callback = function(sTrId, nErrorCode, sErrorMsg)
{
	trace(sTrId+":"+nErrorCode+":"+sErrorMsg);	
}


this.Form_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.fn_displayInfo();

	if(!nexacro._manual && !nexacro._sample)
	{		
		strCategory = this.ds_sample_list.getColumn(1, "category");
		strSubject = this.ds_sample_list.getColumn(1, "subject");
		strSampleFile = this.ds_sample_list.getColumn(1, "id_sample");
		strDocID = this.ds_sample_list.getColumn(1, "id_doc");
		
		this.Div02.form.Grid00.selectRow(1);		
	}
	else
	{
		strSampleFile = nexacro._sample;
		strDocID = nexacro._manual;
		
		strCategory = this.ds_sample_list.getColumn(this.ds_sample_list.findRow("id_sample", strSampleFile), "category");
		strSubject = this.ds_sample_list.getColumn(this.ds_sample_list.findRow("id_sample", strSampleFile), "subject");
	}

	this.fn_LoadContents(strDocID, strSampleFile, strCategory, strSubject);

};

this.Div02_Grid00_oncellclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	strCategory = this.ds_sample_list.getColumn(e.row, "category");
	strSubject = this.ds_sample_list.getColumn(e.row, "subject");
	strSampleFile = this.ds_sample_list.getColumn(e.row, "id_sample");
	strDocID = this.ds_sample_list.getColumn(e.row, "id_doc");

	if((strSampleFile !== undefined) && (strDocID !== undefined))
	{
		this.fn_LoadContents(strDocID, strSampleFile, strCategory, strSubject);
	}	
	
};

this.Div01_ImageViewer00_onclick = function(obj:nexacro.ImageViewer,e:nexacro.ClickEventInfo)
{
	if(this.Div02.width !== 0)
	{
		this.Div02.set_width(0);
	
		this.Div01.set_left(5);
		this.Div01.set_width(880);	
	}
	else
	{
		this.Div02.set_width(240);
	
		this.Div01.set_left(245);
		this.Div01.set_width(640);
	}
};


/*
 * reload sample	
 */
this.Div00_btn_img_reload_onlbuttondown = function(obj:nexacro.ImageViewer,e:nexacro.MouseEventInfo)
{

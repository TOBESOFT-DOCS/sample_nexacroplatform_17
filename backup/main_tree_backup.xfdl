};
this.Div00_btn_img_reload_onmouseleave = function(obj:nexacro.ImageViewer,e:nexacro.MouseEventInfo)
{
	this.Div00.form.btn_img_reload.set_image("imagerc::icon_reload_7a7a7a_32.png");		
};
this.Div00_btn_img_reload_onclick = function(obj:nexacro.ImageViewer,e:nexacro.ClickEventInfo)
{
	this.Div03.form.Tab00.tp_description.set_url("");
	this.Div03.form.Tab00.tp_description.set_url("Sample::" + strSampleFile + ".xfdl");
};


/*
 *	xfdl file download
 */
this.Div00_btn_img_download_file_onlbuttondown = function(obj:nexacro.ImageViewer,e:nexacro.MouseEventInfo)
{
	this.Div00.form.btn_img_download_file.set_image("imagerc::icon_download_red_32.png");
};

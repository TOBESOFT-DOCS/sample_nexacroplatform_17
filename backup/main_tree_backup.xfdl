};
this.Div00_btn_img_download_file_onmouseleave = function(obj:nexacro.ImageViewer,e:nexacro.MouseEventInfo)
{
	this.Div00.form.btn_img_download_file.set_image("imagerc::icon_download_7a7a7a_32.png");	
};
this.Div00_btn_img_download_file_onclick = function(obj:nexacro.ImageViewer,e:nexacro.ClickEventInfo)
{	
	//strURL = nexacro._hostname + "/developer_guide/source/17/Sample/" + nexacro._sample + ".xfdl";
	strURL = g_strAppInfo.serverURL + "/source/17/Sample/" + strSampleFile + ".xfdl";
	
	system.execBrowser(strURL);
};


/*
 *	project files download
 */
this.Div00_btn_img_download_prj_onlbuttondown = function(obj:nexacro.ImageViewer,e:nexacro.MouseEventInfo)
{
	this.Div00.form.btn_img_download_prj.set_image("imagerc::icon_cloud-computing_red_32.png");
};
this.Div00_btn_img_download_prj_onlbuttonup = function(obj:nexacro.ImageViewer,e:nexacro.MouseEventInfo)
{
	this.Div00.form.btn_img_download_prj.set_image("imagerc::icon_cloud-computing_7a7a7a_32.png");
};
this.Div00_btn_img_download_prj_onmouseenter = function(obj:nexacro.ImageViewer,e:nexacro.MouseEventInfo)
{
	this.Div00.form.btn_img_download_prj.set_image("imagerc::icon_cloud-computing_black_32.png");	
};
this.Div00_btn_img_download_prj_onmouseleave = function(obj:nexacro.ImageViewer,e:nexacro.MouseEventInfo)
{
	this.Div00.form.btn_img_download_prj.set_image("imagerc::icon_cloud-computing_7a7a7a_32.png");	
};
this.Div00_btn_img_download_prj_onclick = function(obj:nexacro.ImageViewer,e:nexacro.ClickEventInfo)
{	
	//strURL = nexacro._hostname + "/developer_guide/source/sample_nexacroplatform_17.zip";
	strURL = g_strAppInfo.serverURL + "/source/sample_nexacroplatform_17.zip";

	system.execBrowser(strURL);
};]]></Script>

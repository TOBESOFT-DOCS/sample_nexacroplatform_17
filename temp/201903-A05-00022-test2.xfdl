<?xml version="1.0" encoding="utf-8"?>
<FDL version="2.0">
  <Form id="sample_animationtimeline_01" left="0" top="0" width="1024" height="768" titletext="New Form" onload="sample_animationtimeline_01_onload">
    <Layouts>
      <Layout height="768" width="1024">
        <Static id="Static00" taborder="0" left="27" top="70" width="103" height="43" border="1px solid darkgray" borderRadius="50px" background="gainsboro"/>
        <Button id="Button00" taborder="1" left="26" top="68" width="48" height="47" borderRadius="50px" onclick="Button00_onclick"/>
      </Layout>
    </Layouts>
    <Objects/>
    <Script type="xscript5.1"><![CDATA[this.nOnOff = 0;

this.sample_animationtimeline_01_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
    var aniObj00 = new nexacro.Animation("Ani00", this);
    this.addChild("Ani00", aniObj00);
    this.Ani00.addTarget("AniItem00", this.Button00, "left:27");
	this.Ani00.set_duration(200);
	
    var aniObj01 = new nexacro.Animation("Ani01", this);
    this.addChild("Ani01", aniObj01);
    this.Ani01.addTarget("AniItem00", this.Button00, "left:85");
	this.Ani01.set_duration(200);
};

this.Button00_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if(this.nOnOff == 0){
		this.Ani01.play();
		this.Static00.set_background("aliceblue");
		this.nOnOff = 1;
	}else{
		this.Ani00.play();
		this.Static00.set_background("gainsboro");
		this.nOnOff = 0;
	}
};
]]></Script>
  </Form>
</FDL>
